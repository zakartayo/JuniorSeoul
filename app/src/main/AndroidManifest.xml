        <activity
            android:name=".KidsCafeDetailActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".KidsCafeListActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MuseumArtActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MuseumArtListActivity"
            android:screenOrientation="portrait" />
